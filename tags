!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
<C-k>	init.vim	/^imap <C-k>     <Plug>(neosnippet_expand_or_jump)$/;"	m
<C-x>	vim_config/maps.vim	/^nnoremap <C-x> :set nonumber!<CR>$/;"	m
<Leader>"	vim_config/maps.vim	/^nnoremap <Leader>" ciw""<Esc>P$/;"	m
<Leader>'	vim_config/maps.vim	/^nnoremap <Leader>' ciw''<Esc>P$/;"	m
<Leader>ag	vim_config/maps.vim	/^nnoremap <Leader>ag :Ag<SPACE>$/;"	m
<Leader>b	vim_config/maps.vim	/^nnoremap <Leader>b :<C-u>call gitblame#echo()<CR>$/;"	m
<Leader>bc	vim_config/maps.vim	/^map <Leader>bc :BCommits!<CR> $/;"	m
<Leader>c	vim_config/maps.vim	/^map <Leader>c <c-_><c-_>$/;"	m
<Leader>ca	vim_config/maps.vim	/^nnoremap <Leader>ca :%bd<CR>$/;"	m
<Leader>cc	vim_config/maps.vim	/^map <Leader>cc  <c-_>b $/;"	m
<Leader>f	vim_config/maps.vim	/^map <Leader>f :Files!<CR> $/;"	m
<Leader>h	vim_config/maps.vim	/^nmap <Leader>h :History<CR> $/;"	m
<Leader>i	vim_config/maps.vim	/^nmap <Leader>i gg=G`.$/;"	m
<Leader>l	vim_config/maps.vim	/^nmap <Leader>l `.$/;"	m
<Leader>nf	vim_config/maps.vim	/^map <Leader>nf :NERDTreeFind<CR>$/;"	m
<Leader>nt	vim_config/maps.vim	/^map <Leader>nt :NERDTreeToggle<CR>$/;"	m
<Leader>p	vim_config/maps.vim	/^nmap <Leader>p  <Plug>(coc-format-selected)$/;"	m
<Leader>q	vim_config/maps.vim	/^nnoremap <Leader>q :q<CR>$/;"	m
<Leader>w	vim_config/maps.vim	/^nnoremap <Leader>w :w<CR>$/;"	m
<S-o>	vim_config/maps.vim	/^nmap <S-o> i<Enter><Esc>k<cr>$/;"	m
K	init.vim	/^nnoremap <silent> K :call <SID>show_documentation()<CR>$/;"	m
NERDTreeCascadeSingleChildDir	init.vim	/^let NERDTreeCascadeSingleChildDir=0$/;"	v
NERDTreeShowHidden	init.vim	/^let NERDTreeShowHidden=1 "NerdTree plugins shows hidden files$/;"	v
Prettier	init.vim	/^command! -nargs=0 Prettier :CocCommand prettier.formatFile$/;"	c
g:NERDTreeWinSize	init.vim	/^let g:NERDTreeWinSize=35 "NerdTree width$/;"	v
g:ag_prg	init.vim	/^let g:ag_prg="<custom-ag-path-goes-here> --vimgrep"$/;"	v
g:airline#extensions#tabline#enabled	init.vim	/^let g:airline#extensions#tabline#enabled = 1$/;"	v
g:airline#extensions#tabline#fnamemod	init.vim	/^let g:airline#extensions#tabline#fnamemod = ':t'"$/;"	v
g:airline_powerline_fonts	init.vim	/^let g:airline_powerline_fonts = 1$/;"	v
g:airline_symbols	init.vim	/^  let g:airline_symbols = {}$/;"	v
g:airline_theme	init.vim	/^let g:airline_theme='owo'$/;"	v
g:fzf_commits_log_options	init.vim	/^let g:fzf_commits_log_options = '--graph --color=always --format="%C(auto)%h%d %s %C(black)%C(bold)%cr"'$/;"	v
g:fzf_preview_window	init.vim	/^let g:fzf_preview_window = 'right:60%'$/;"	v
g:gitgutter_max_signs	init.vim	/^let g:gitgutter_max_signs = 500  " default value (Vim < 8.1.0614, Neovim < 0.4.0)$/;"	v
g:move_key_modifier	init.vim	/^let g:move_key_modifier = 'C'$/;"	v
g:neosnippet#enable_snipmate_compatibility	init.vim	/^let g:neosnippet#enable_snipmate_compatibility = 1$/;"	v
g:neosnippet#snippets_directory	init.vim	/^let g:neosnippet#snippets_directory='~\/.local\/share\/nvim\/snippets'$/;"	v
gd	vim_config/maps.vim	/^nmap <silent> gd <Plug>(coc-definition)$/;"	m
gi	vim_config/maps.vim	/^nmap <silent> gi <Plug>(coc-implementation)$/;"	m
gr	vim_config/maps.vim	/^nmap <silent> gr <Plug>(coc-references)$/;"	m
gy	vim_config/maps.vim	/^nmap <silent> gy <Plug>(coc-type-definition)$/;"	m
jj	vim_config/maps.vim	/^imap jj <Esc>$/;"	m
mapleader	vim_config/maps.vim	/^let mapleader=" "$/;"	v
show_documentation	init.vim	/^function! s:show_documentation()$/;"	f
·	vim_config/maps.vim	/^nnoremap · :BufExplorer<CR>$/;"	m
